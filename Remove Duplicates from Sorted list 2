# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, val=0, next=None):
#         self.val = val
#         self.next = next
class Solution:
    def deleteDuplicates(self, head: Optional[ListNode]) -> Optional[ListNode]:
        temp = head
        while True :
            if temp and temp.next and temp.val==temp.next.val:
                temp_val=temp.val
                while temp and temp.val == temp_val:
                    temp=temp.next
            else:
                break
        head = temp
        prev =None
        while temp :
            if temp and temp.next and temp.val == temp.next.val :
                temp_val=temp.val
                while temp and temp.val == temp_val :
                    temp=temp.next
                prev.next=temp
            else:
                prev=temp
                temp=temp.next
            
        return head
            
        
